{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "@/*": ["./src/*"]
        },
        "types": ["vite/client"]
    },
    "include": ["./src/**/*.ts", "./src/**/*.d.ts", "./src/**/*.vue"],
}
